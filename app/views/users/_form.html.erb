<%= simple_form_for @user do |f| %>
  <%= f.input :username %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :date_of_birth %>
  <%= f.input :bio, as: :text %>
  <%= f.input :phone_number %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>
