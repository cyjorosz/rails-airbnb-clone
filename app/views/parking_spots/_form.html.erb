<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
    <%= simple_form_for @parking_spot do |f| %>
      <%= f.error_notification %>
      <%= f.input :size %>
      <%= f.input :price_per_day %>
<!-- address input -->
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>




